# Sui Network Configuration
NEXT_PUBLIC_NETWORK=mainnet

# Contract Addresses (deploy to mainnet before production)
NEXT_PUBLIC_PACKAGE_ID=0x0
NEXT_PUBLIC_STATS_OBJECT_ID=0x0
NEXT_PUBLIC_MARKETPLACE_ID=0x0
NEXT_PUBLIC_REWARD_POOL_ID=0x0

# Feature Flags
NEXT_PUBLIC_USE_BLOCKCHAIN=true
NEXT_PUBLIC_USE_FREESOUND=true
# SEAL Key Server (deployed via railway/ folder)
# After Railway deployment, set to your deployed key server URL
NEXT_PUBLIC_SEAL_KEY_SERVERS=https://seal.projectsonar.xyz

# API Configuration
NEXT_PUBLIC_RPC_URL=https://fullnode.mainnet.sui.io
NEXT_PUBLIC_GRAPHQL_URL=https://sui-mainnet.mystenlabs.com/graphql
NEXT_PUBLIC_WALRUS_PUBLISHER_URL=https://publisher.walrus-testnet.walrus.space
NEXT_PUBLIC_WALRUS_AGGREGATOR_URL=https://aggregator.walrus-testnet.walrus.space

# Backend API (deploy backend before enabling)
NEXT_PUBLIC_BACKEND_URL=

# Audio Checker Service (Railway Python service)
AUDIO_CHECKER_URL=https://your-audio-checker.railway.app

# External API Tokens (server-only)
# Get Freesound token at: https://freesound.org/apiv2/apply
# FREESOUND_API_TOKEN=your_freesound_token
# WALRUS_SIGNING_KEY=your_walrus_key
# WALRUS_UPLOAD_ENDPOINT=https://api.walrus.testnet/upload
# WALRUS_SEAL_POLICY_ID=seal_policy_id
# VERIFICATION_AI_MODEL=gpt-4o-mini
